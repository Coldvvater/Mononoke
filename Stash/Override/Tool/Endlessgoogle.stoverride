name: Google无缝翻页
desc: 谷歌网页搜索自动无缝翻页
author: tumpio
icon: "https://github.com/Koolson/Qure/blob/master/IconSet/Color/Google.png?raw=true"
category: "Coldvvater"

http:
  mitm:
    - "www.google.*"
  script:
    - match: ^https?:\/\/www\.google\..*\/search
      name: Google
      type: response
      require-body: true
      timeout: 10
script-providers:
  Google:
    url: https://raw.githubusercontent.com/langkhach270389/Surge-LK/main/scripts/langkhach/endlessgoogle.js
    interval: 86400
