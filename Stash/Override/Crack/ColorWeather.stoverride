name: 彩云天气SVIP
desc: 彩云天气会员解锁
#!author= ddgksf2013

http:
  rewrite:
    - ^https:\/\/ad\.caiyunapp\.com\/ - reject

  script:
    - match: ^http[s]?:\/\/biz\.(caiyunapp|cyapi)\.(com|cn)\/v2\/user.*$
      name: cytq
      type: response
      timeout: 30
      require-body: true
      max-size: 3145728

  mitm:
    - "*.caiyunapp.*"
    - "*cyapi*"

script-providers:
  cytq:
    url: https://raw.githubusercontent.com/WeiRen0/Scripts/main/cytq.js
    interval: 86400
