name: 去广告规则
desc: 分流去广告

rule-providers:
  Adblock:
    type: http
    behavior: domain-text
    url: "https://ghproxy.com/https://raw.githubusercontent.com/privacy-protection-tools/anti-AD/master/anti-ad-surge.txt"
    path: ./ruleset/Adblock.yaml
    interval: 86400

rules:
  - RULE-SET,Adblock,REJECT
